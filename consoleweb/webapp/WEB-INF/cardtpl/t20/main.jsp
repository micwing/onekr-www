		 }
		 return ;
	 }
	 if (content.length<2){
		 jQuery.dialog.tips('<p class="tips_12_g">留言内容不能少于2个字符</p>');
		 jQuery('#content').focus();
		 return ;
	 }
	 $.ajax({
		'type':'post',
