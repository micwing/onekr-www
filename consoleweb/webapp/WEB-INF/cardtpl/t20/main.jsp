			if (json.status=='1'){
				jQuery.dialog.tips('<p class="tips_12_g">'+json.msg+'</p>');
				$("#msg_area").prepend(json.html).hide().fadeIn("slow");
			}else{
				
				jQuery.dialog.tips('<p class="tips_12_g">'+json.msg+'</p>');
			}
		}
	 });
	}
function get_wall(p){
	if (p>0){
